{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    ["import", {
      "libraryName": "antd",
      "libraryDirectory": "lib",
      "style": true
      //"style": "css"
    }
    ],
    [
      "styled-components",
      {
        // Enable or disable server side rendering (SSR)
        "ssr": false,
        "displayName": true,
        "preprocess": false
      }
    ]
  ]
}